    <h1 class="center">Express web server</h1>
    <div class="login">
        <p id="bienvenido">Bienvenido<strong>{{{email}}}</strong></p>
        
        <form id="submit" action="/logout" method="POST">
            <input type="hidden" name="email" value="{{email}}">
            <button role="button" type="submit" id="submit-username">Desloguear</button>
        </form>

    </div>
    <main class="container">
        <section>
            <h3>Agregar Producto</h3>

            <form>
                <input type="text" name="title" id="title-product" placeholder="Titulo" required>
                <input type="text" name="price" id="price-product" placeholder="Precio" required>
                <input type="text" name="thumbnail" id="thumbnail-product" placeholder="URL de imagen" required>
                <button style="width: 20%; align-self: flex-end" role="button" type="submit"
                    id="submit-products">Agregar</button>
            </form>
        </section>
        <section>
            <h3>Productos</h3>
            <table id="tabla-productos">
                <thead>
                    <tr>
                        <th>Nombre</th>
                        <th>Precio</th>
                        <th>Foto</th>
                    </tr>
                </thead>
                <tbody id="tbody-productos">
                    <!-- id="tbody-productos" -->

                </tbody>

            </table>
        </section>

        <section>
            <div class="compresion">
                <h4>Centro de mensajes</h4>
                <p id="compresion"></p>
            </div>
            <form id="form-message">
                <span id="validEmail"></span>
                <input type="email" name="input-email" id="input-email" placeholder="Email">
                <input type="text" name="input-name" id="input-name" placeholder="Nombre">
                <input type="text" name="input-lastname" id="input-lastname" placeholder="Apellido">
                <input type="number" name="input-age" id="input-age" placeholder="Edad">
                <input type="text" name="input-alias" id="input-alias" placeholder="Alias">
                <input type="text" name="input-avatar" id="input-avatar" placeholder="URL Avatar">
                <input type="text" name="input-message" id="input-message" placeholder="Escribe un mensaje">
                <button type="submit">Enviar</button>
            </form>

            <h5>Historial de mensajes</h1>
                <ul id="show-message"></ul>
        </section>

        <section>
            <a href="./api/productos-test"><button>Faker data</button></a>
        </section>
    </main>


    <script src="https://cdn.jsdelivr.net/npm/normalizr@3.6.1/dist/normalizr.browser.min.js"></script>
