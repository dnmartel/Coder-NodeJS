<h2>Productos</h2>

<table>
    <tr>
        <th>Nombre</th>
        <th>Precio</th>
        <th>Foto</th>
    </tr>

    {{#each data}}
        <tr>
            <td>{{this.title}}</td>
            <td>{{this.price}}</td>
            <td><img src="{{this.thumbnail}}" alt="{{this.title}}" width="50px" height="50px"></td>
        </tr>
    {{else}}
        <h3 style="text-align: center;">No hay productos en el listado</h3>
    {{/each}}
    


</table>
            
<a href="http://localhost:8080/"><button>Al formulario</button></a>    
